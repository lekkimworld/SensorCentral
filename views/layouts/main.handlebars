<script type="text/javascript" src="/js/bootstrap-datetimepicker.min.js"></script>
<script type="text/javascript" src="/js/index.js"></script>

<nav class="navbar navbar-expand-md navbar-dark bg-dark fixed-top">
  <span class="fa fa-thermometer-empty fa-2x color-white" aria-hidden="true">&nbsp;</span>
  <span class="d-none d-lg-block d-md-block">
    <a class="navbar-brand" href="#">SensorCentral</a>
  </span>
  <span class="d-sm-block d-md-none">
    <a class="navbar-brand" href="#">SensorCentral</a>
  </span>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  
  <div class="collapse navbar-collapse" id="navbarsExampleDefault">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item {{home_active}}">
        <a class="nav-link" href="/">Home</a>
      </li>
      <li class="nav-item {{config_active}}">
        <a class="nav-link" href="/configuration">Configuration</a>
      </li>
      <li class="nav-item {{dash_active}}">
        <a class="nav-link" href="/dashboard">Dashboard</a>
      </li>
      <li class="nav-item {{about_active}}">
          <a href="/about" class="nav-link">About</a>
      </li>
    </ul>
    <ul class="nav navbar-nav">
      <a class="nav-link dropdown-toggle" href="#" id="navbarUsernameLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
