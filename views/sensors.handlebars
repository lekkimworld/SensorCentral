<h1>{{title}}</h1>
<span class="clearfix">
    <i class="float-left">Updated: {{updated}}</i>
    <a href="" class="float-right color-black"><i class="fa fa-sync fa-2x" aria-hidden="true"></i></a>
</span>
<div class="row">
    {{#each data}}
    <div class="col-lg-2 col-md-3 col-sm-4 sensorColumn"><div class="sensorReading">
        <div class="value">{{this.value_string}}</div>
        {{#if this.name}}
            <div class="name">{{this.name}} ({{this.device.name}})</div>
        {{else}}
            <div class="name">{{this.id}}</div>
        {{/if}}
        <div class="name">{{this.ageMinutes}} min. siden</div>
        <div class="name"><a href="/api/v1/excel/{{this.label}}/last24hrs/5m">Export last 24 hrs (5 min resolution)</a></div>
        <div class="name"><a href="/api/v1/excel/{{this.label}}/last24hrs/60m">Export last 24 hrs (1 hrs resolution)</a></div>
        <div class="name"><a href="/api/v1/excel/{{this.label}}/lastweek">Export last week</a></div>
    </div></div>
    {{/each}}    
</div>