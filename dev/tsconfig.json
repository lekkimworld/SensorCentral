{
  "extends": "../tsconfig.json",
  "compilerOptions": {
      "module": "es2015",
      "lib": ["es2016", "dom"],
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "noImplicitAny": false,
      "noUnusedLocals": false,
